{
  "name": "@poly/styles",
  "version": "1.0.0",
  "description": "Main Poly styles package",
  "keywords": [
    "styles"
  ],
  "author": "Nitin <nitinpanwar0802@gmail.com>",
  "homepage": "https://github.com/Polyorg/system-design.git#readme",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Polyorg/system-design.git",
    "directory": "packages/styles"
  },
  "scripts": {
    "build": "tsup src/index.ts --format=esm,cjs --dts",
    "dev": "pnpm build -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src/index.ts --format=esm,cjs"
  },
  "dependencies": {
    "@poly/common": "workspace:*",
    "@poly/theme": "workspace:*"
  }
}
